{
    "name" : "karimnewpackage/karimnewpackage",
    "description": "karim first package",
    "type": "project",
    "keywords": ["framework", "karim"],
    "license": "MIT",
    "require": {
        "php" :  "^8.0.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
